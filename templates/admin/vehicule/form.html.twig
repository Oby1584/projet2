{% extends "base.html.twig" %}

{% block title %}ajout voiture{% endblock %}

{% block body %}
    <h1 class="text-center my-4" >Ajout Voiture</h1>
<div class="container">
    {{ form_start(form)}}
        {{form_row(form.marque)}}
        {{form_row(form.modele)}}
        {{form_row(form.prix_journalier)}}
        {{form_row(form.titre)}}
        {{form_row(form.photo)}}
        {{form_row(form.description)}}
        {# {{form_row(form.date_enregistrement)}} #}
        <button type="submit" class="btn btn-primary" >Ajouter</button>


    {{ form_end(form)}}
</div>

{% endblock %}